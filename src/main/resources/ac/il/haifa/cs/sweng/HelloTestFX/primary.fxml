<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox id="body" alignment="CENTER" prefHeight="500.0" prefWidth="590.0" spacing="20.0" styleClass="body" stylesheets="@style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ac.il.haifa.cs.sweng.HelloTestFX.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <TextField id="textField" fx:id="textField" editable="false" />
      <Button fx:id="sayHello" mnemonicParsing="false" onAction="#sayHello" text="Say Hello" textFill="WHITE" />
   </children>
</VBox>
